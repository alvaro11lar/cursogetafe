CREATE table TB_VEHIVULOS_GUERRA(
	id INTEGER NOT NULL AUTO_INCREMENT PRIMARY KEY, 
	nombre VARCHAR(20) NOT NULL,
	ataque DECIMAL(3,2),
	defensa DECIMAL(3,2),
	tipo VARCHAR(30)
);

CREATE table TB_GUERRERO(
	id INTEGER NOT NULL AUTO_INCREMENT PRIMARY KEY ,
	nombre VARCHAR(20) NOT NULL,
	tipo VARCHAR(30),
	fuerza DECIMAL(3,2),
	resistencia DECIMAL(3,2),
	vehiculo_id INTEGER NOT NULL
	
);

